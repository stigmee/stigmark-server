{
    "name": "Stigmark",
    "version": "1.1",
    "manifest_version": 3,
    "icons": {
        "48": "icons/BMButton.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.htm"
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage"
    ]
}